         switch
                var my_data=prompt('');
                switch (my_data) {
                    case 'Mohammad':
                        first_div.innerText= my_data +" Yor are Administrator";
                    break;
                    case 'Ali':
                        first_div.innerText=my_data+"Yor are admin";
                        break;
                    default:
                        first_div.innerText=my_data+"Yor are nothing";
                }
